{
  "name": "schwigri/karma",
  "description": "",
  "type": "package",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/schwigri/karma",
  "authors": [
    {
      "name": "Contributors",
      "homepage": "https://github.com/schwigri/karma/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "wptrt/wpthemereview": "^0.2.0"
  },
  "scripts": {
    "lint": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
    "checkcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs --runtime-set testVersion 5.6-",
    "fixcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf --runtime-set testVersion 5.6-"
  },
  "support": {
    "issues": "https://github.com/schwigri/karma/issues",
    "source": "https://github.com/schwigri/karma"
  }
}
